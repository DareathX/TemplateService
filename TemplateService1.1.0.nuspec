<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>TemplateService</id>
    <version>1.1.0</version>
    <title></title>
    <authors>Wilco Ram</authors>
    <owners>Netcompany</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Template service which accepts Word and Html templates. Word can be converted to Pdf.</description>
    <dependencies>
      <dependency id="DocumentFormat.OpenXml" version="2.17.1" />
      <dependency id="OpenXmlPowerTools" version="4.5.3.2" />
    </dependencies>
    <contentFiles>
      <files include="cs\any\**\*.*" buildAction="None" copyToOutput="true" flatten="false" />
    </contentFiles>
  </metadata>
  <files>
    <file src="contentFiles\cs\any\**\*.*" target="contentFiles\cs\any" />
    <file src="lib\net6.0\TemplateService.dll" target="lib\net6.0\TemplateService.dll" />
  </files>
</package>